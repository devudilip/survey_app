<div class="page-header">
  <h2> <%= @survey.name  %>: Users List</h2>
</div>
<% if @users.blank? %>
There are no responses for survey <%= @survey.name  %> <br />
<% else %>
<table class="table table-striped">
  <thead>
    <tr>
      <th> User Name </th>
      <th> Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.user_name, user_survey_survey_path(@survey,user_id: user.id) %></td>
      <td><%= user.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<div class="apple_pagination">
  <%= will_paginate @users %>
</div>
<% end %>
<%= link_to "Back To Surveys",surveys_path,:class => 'btn btn-primary' %>